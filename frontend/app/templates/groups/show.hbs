<h2 id="group-title"> {{group.grade}} {{group.name}} {{group.year}}</h2>
<nav id="group-nav">
  {{#each group.worksheetSets as |set|}}
    {{link-to set.title 'groups.show.worksheet_set' group.id set.id }}
  {{/each}}
   {{link-to 'Erstelle ein neues Set' 'groups.show.create_set'}}
</nav>

<div class="content-wrapper">
  {{#each group.statistics as |stat|}}
    {{stat.positives}}
  {{/each}}
  {{outlet}}
</div>
