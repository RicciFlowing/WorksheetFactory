<h2> Ergebnisse f√ºr {{worksheet.title}} der {{group.grade}}{{group.name}} </h2>
<form>
<ul class="list-unstyled">

  {{#each worksheet.questions as |question index|}}
    <li>
      <p>
      {{k-tex question.text}} = {{k-tex question.solution}}
      </p>
      <div class="form-inline">
        <div class="form-group">
          <label> Richtige</label>
          {{input type="number" value = question.positives class="form-control"}}
        </div>

        <div class="form-group">
          <label> Falsche</label>
          {{input type="number" value = question.negatives class="form-control"}}
        </div>
      </div>
    </li>
  {{/each}}
</ul>

<div class="form-group">
  <button type="button" {{action 'submit'}} class='btn btn-default'> Ergebnisse erfassen </button>
</div>
</form>
