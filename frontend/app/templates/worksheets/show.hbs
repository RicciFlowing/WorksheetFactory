<h3>{{worksheet.title}} </h3>

<div id="worksheetTable">
  <table class="table">
    <thead>
      <td>
        #
      </td>
      <td>
        Question
      </td>
      <td>
        Solution
      </td>
    </thead>
<tbody>
    {{#each questions as |question index|}}
      {{question-row question=question index=index}}
    {{/each}}
</tbody>
  </table>
</div>

{{#link-to 'groups.show' group.id}} Back to {{group.name}} {{/link-to}}
